package day02;

public class LearnClass {
	
	// 9489052534
	//Class. Variable, Method and Object

	/* Java: Static Data type programming language
	 * *Class:
	 * 
	 *    -Variable: Data or information about that class.
	 *        DataType variableName= Value;
	 *        
	 *    -Method : Find the action of the class
	 *    
	 *    Method Signature:
	 *    
	 *    Syntax:
	 *    
	 *    accessModifier returnType methodName() {
	 *    
	 *    //Body of the Method
	 *    
	 *    }
	 *    
	 *    AccessModifer:(Method)
	 *    
	 *    4 type of Access Modifier:
	 *    
	 *       -public                 | Class, Package, Global
	 *       -default or No Modifer  | Class, Package
	 *       -protected
	 *       -private
	 *    
	 *    -Constructor
	 *    
	 */
	public void method1() {
		System.out.println ("Method 01");
	}
	
	void method2() {
		System.out.println ("Method 02");
	}
	
	private void method3() {
		System.out.println ("Method 03");
	}
	public static void main(String[] args) {
		LearnClass lc= new LearnClass();
		lc.method1();
		lc.method2();
		lc.method3();
		
	}
	pubic String getName() {
		return "priyanka";		
	}
	public int getNum() {
		int a= 100;
		int b=200;
		int c=a+b;
		return c;
				
		//return 12345;		
	}
}
